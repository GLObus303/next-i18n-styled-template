{
  "name": "next-i18n-styled-template",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "type-check:prod": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc --build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "next": "^10.0.4",
    "next-i18next": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/node": "^14.14.16",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "@types/react": "^17.0.2",
    "autoprefixer": "^10.2.4",
    "eslint": "7.20.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    ".": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged && npm run type-check:prod"
    }
  }
}
